+ifeq ($(NOFORTRAN), $(filter 0,$(NOFORTRAN)))
+	if [ ! -a $< ]; then
+	-wget http://www.netlib.org/lapack/timing/timing.tgz;
+	fi
+endif
+
+lapack-timing : large.tgz timing.tgz
+ifeq ($(NOFORTRAN), $(filter 0,$(NOFORTRAN)))
+	(cd $(NETLIB_LAPACK_DIR); $(TAR) zxf ../timing.tgz TIMING)
